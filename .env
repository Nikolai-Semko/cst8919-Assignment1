# Auth0 Configuration
# Get these values from your Auth0 Application Dashboard
AUTH0_CLIENT_ID=your_auth0_client_id_here
AUTH0_CLIENT_SECRET=your_auth0_client_secret_here
AUTH0_DOMAIN=your_auth0_domain.auth0.com

# Flask Application Configuration
APP_SECRET_KEY=your_random_secret_key_here_generate_with_python_secrets

# Development Settings (optional)
FLASK_DEBUG=True
PORT=3000

# Example values (DO NOT USE IN PRODUCTION):
# AUTH0_CLIENT_ID=ABC123def456ghi789
# AUTH0_CLIENT_SECRET=XYZ987wvu654tsrq321-_AbCdEfGhIjKlMn
# AUTH0_DOMAIN=dev-example.us.auth0.com
# APP_SECRET_KEY=super-secret-key-change-this-in-production